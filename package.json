{
	"name": "@subhamx/notion-to-ebook",
	"version": "1.0.4",
	"description": "Notion to ebook is a lightweight library to convert notion document consisting of multiple nested documents or databases into an ebook format like PDFs, with the table of contents, page cover, headers etc.",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"scripts": {
		"watch": "tsc -w",
		"build": "tsc -b",
		"test": "node lib/tests/index.js"
	},
	"keywords": [
		"notion to ebook",
		"notion",
		"notion to pdf",
		"bulk export",
		"notion api",
		"merge pdf",
		"pdf",
		"book",
		"kindle"
	],
	"author": "subhamx",
	"license": "Apache-2.0",
	"dependencies": {
		"cheerio": "^1.0.0-rc.10",
		"extract-zip": "^2.0.1",
		"fs-extra": "^10.0.0",
		"node-fetch": "^2.6.1",
		"winston": "^3.3.3"
	},
	"devDependencies": {
		"@types/fs-extra": "^9.0.12",
		"@types/node": "^16.4.3",
		"@types/node-fetch": "^2.5.12",
		"typescript": "^4.3.5"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/subhamX/notion-to-ebook"
	},
	"publishConfig": {
		"@YOUR-USERNAME:registry": "https://npm.pkg.github.com"
	  }
}
